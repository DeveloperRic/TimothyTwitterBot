    <children>
        <Label text="Timothy needs access" GridPane.columnSpan="2">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
      <Label text="Enter the code from twitter  " GridPane.rowIndex="1" />
        <TextField fx:id="Setup_twitterCode" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Button alignment="CENTER" mnemonicParsing="false" onAction="#onSetupVerifyClick" text="Verify" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="2" />
    </children>
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints />
      <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
    </rowConstraints>
    <padding>
        <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
    </padding>
